		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-tailwindcss": "^3.7.1",
		"next": "13.1.6",
		"prettier": "^2.8.2",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"typescript": "4.9.4"
	},
